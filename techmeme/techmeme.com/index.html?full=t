h_e.style.backgroundColor=hor+(hog-phase*16)+')';
h_t = setTimeout('h(0,'+(phase+1)+')',100);
} else if (phase < 12) {
h_e.style.backgroundColor=hor+(hog-6*16+(phase-6)*16)+')';
var tms = 100;
if (phase == 6) { tms = 1000; }
h_t = setTimeout('h(0,'+(phase+1)+')',tms);
} else if (phase == 12) {
h_e.style.backgroundColor = hoc;
h_t=null;
}
