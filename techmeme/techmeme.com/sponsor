alert("Sorry, no page exists yet for that date");
} else if (m2_ad < m1_sad) {
alert(em);
} else if (m2_ad < m2_sad) {
location = arrt+m1_ad;
} else {
location = arrt+m2_ad;
}
} else {
alert('Please use this format:  '+pgrdad);
}
}
