return this.Ts + " " + this.Td;
}
}
TE.Tc = function (a,b) {
return a.Td - b.Td
}
var TcE_ = null;
function TgXMLHR() {
var Tx = false;
if (window.XMLHttpRequest) {
Tx = new XMLHttpRequest();
} else {
try
{
Tx = new ActiveXObject("Msxml2.XMLHTTP");
}
catch (ev)
{
try
{
Tx = new ActiveXObject("Microsoft.XMLHTTP");
}
catch (ev)
{
Tx = false;
}
}
}
return Tx;
}
function TeD() {
