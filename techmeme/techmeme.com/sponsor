h+=12;
} else if ((fa[4]=='am'||fa[5]=='am') && h==12) {
h=0;
}
fa[3]=h+'';
fa[4]=m+'';
if (
(fa[0]>=1 && fa[0]<=12) &&
(fa[1]>=1 && fa[1]<=31) &&
(fa[2]>=1 && fa[2]<=curr_yy) &&
(fa[4]>=0 && fa[4]<=59)
) {
