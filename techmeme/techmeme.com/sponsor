if (TcE_.Tx) {
TcE_.Tx.open('POST', TsTS+'?tm=true', true);
TcE_.Tx.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
TcE_.Tx.onreadystatechange = TeC;
