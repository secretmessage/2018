fa[2] = curr_yy+'';
tpyy=((100+curr_yy-1)+'').substring(1,3);
}
if (fa[2] > 1900) {
if (fa[2] < 2001) {
alert(em);
return;
}
fa[2] = fa[2].substring(2,4);
} else if (fa[2] > 80) {
alert(em);
return;
}
var h,m;
if (fa[3]=='0' || fa[3]=='00' || (fa[0]>=1 && fa[3]<=23)) {
h=parseInt(fa[3],10);
m=parseInt(fa[4],10);
if (!m) { m=0; }
} else {
h=20;
m=0;
}
m=m-m%min_mod;
